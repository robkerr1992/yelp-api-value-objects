{
    "name": "yeet-labs/yelp-value-objects",
    "description": "Value objects for interacting with the stevenmaguire/yelp-php Yelp API wrapper",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "~7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0@dev",
        "phpunit/phpunit": "~8.1",
        "symfony/var-dumper": "^4.3@dev"
    },
    "autoload": {
        "psr-4": {
            "YeetLabs\\YelpValue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YeetLabs\\YelpValue\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "coverage": "./vendor/bin/phpunit --coverage-html build/coverage"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2.29"
        }
    },
    "minimum-stability": "dev"
}
